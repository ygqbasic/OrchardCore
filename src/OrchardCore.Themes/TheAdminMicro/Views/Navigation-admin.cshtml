@{
    TagBuilder tag = Tag(Model, "ul");

    tag.Attributes["lay-filter"] = "test";
    tag.AddCssClass("layui-nav layui-nav-tree");
    foreach (var item in Model)
    {
        tag.InnerHtml.AppendHtml(await DisplayAsync(item));
    }
}


    @tag

